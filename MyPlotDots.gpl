#! /usr/bin/gnuplot -persist
#set terminal postscript eps color solid 22
set terminal png size 1024, 768 
set encoding default
set nokey
set xlabel "x" 
set xrange [ -1.5 : 1.5 ] noreverse nowriteback
set ylabel "z" 
set yrange [ -1.0 : 2.0 ] noreverse nowriteback
set output out_file
plot in_file with dots
